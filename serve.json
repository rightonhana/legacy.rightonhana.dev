{
  "cleanUrls": true,
  "directoryListing": false,
  "headers": [
    {
      "source": "**/*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=31557600"
        },
        {
          "key": "content-security-policy",
          "value": "connect-src 'self'; default-src 'self'; font-src 'none'; frame-src 'none'; img-src 'self'; manifest-src 'self'; media-src 'self'; object-src 'none'; script-src 'self' 'sha256-mFebBjayq9LGonWAmlQ+IZTcdUntfCbm2GknnEBazLU=' storage.googleapis.com; style-src 'self' 'unsafe-inline'; worker-src 'self'"
        },
        {
          "key": "pragma",
          "value": "public"
        },
        {
          "key": "x-content-type-options",
          "value": "nosniff"
        },
        {
          "key": "x-dns-prefetch-control",
          "value": "off"
        },
        {
          "key": "x-download-options",
          "value": "noopen"
        },
        {
          "key": "x-frame-options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "x-xss-protection",
          "value": "1; mode=block"
        }
      ]
    }
  ],
  "public": "./build"
}
